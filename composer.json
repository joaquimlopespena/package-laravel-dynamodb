{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "joaquim/laravel-dynamodb",
    "description": "DynamoDB driver for Laravel with automatic index resolution and Eloquent support",
    "keywords": [
        "laravel",
        "dynamodb",
        "aws",
        "database",
        "driver",
        "eloquent",
        "index",
        "gsi",
        "lsi"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Joaquim",
            "email": "joaquim@example.com"
        }
    ],
    "require": {
      "php": "^8.2",
      "illuminate/database": "^11.0|^12.0",
      "illuminate/support": "^11.0|^12.0",
      "aws/aws-sdk-php": "^3.322.9"
    },
    "require-dev": {
      "orchestra/testbench": "^9.0|^10.0",
      "phpunit/phpunit": "^10.5|^11.0",
      "laravel/pint": "^1.24"
    },
    "autoload": {
        "psr-4": {
            "Joaquim\\LaravelDynamoDb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joaquim\\LaravelDynamoDb\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joaquim\\LaravelDynamoDb\\DynamoDbServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
